public class TripleDice extends Dice {

  static int totalDiceRolled;
	
	public void TrippleDice()
	{
		super.roll_1_dice(3);
	}
	
	public String RollCountMsg()
	  {
		String msg;
		msg = "Calling my super\nYou have rolled the triple dice " + super.RollCount() + " times";
		return msg;
	  }
	
	public void reset()
	{
		super.reset();
	}
	
}
